<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <group>
    <executable cmd="$(env AUTOWARE_CARLA_ROOT)/external/zenoh_carla_bridge/target/release/zenoh_carla_bridge" output="screen">
      <env name="RUST_LOG" value="z=info"/>
    </executable>
    <executable
      cmd="poetry run python3 main.py --host $(env CARLA_SIMULATOR_IP) --rolename 'v1' --position 140.00,133,0.300000,0.000000,0.000000,0.000000"
      cwd="$(env AUTOWARE_CARLA_ROOT)/external/zenoh_carla_bridge/carla_agent" 
      output="screen" />
    <executable
      cmd="poetry run python3 main.py --host $(env CARLA_SIMULATOR_IP) --rolename 'v2' --position 180.00,133,0.300000,0.000000,0.000000,0.000000"
      cwd="$(env AUTOWARE_CARLA_ROOT)/external/zenoh_carla_bridge/carla_agent" 
      output="screen" />
  </group>
</launch>
